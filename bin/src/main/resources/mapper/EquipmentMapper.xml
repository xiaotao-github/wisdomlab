<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fengbiaoedu.mapper.EquipmentMapper">
	<insert id="insertSelective" parameterType="Equipment">
		INSERT into
		equipment(`equ_id`,`gw_id`,`dev_id`,`ep`,`ep_type`,`ep_name`,`seat_num`,`lab_ep_type`,`stealth`,`create_time`,`update_time`)
		VALUES(null,#{gwId},#{devId},#{ep},#{epType},#{epName},#{seatNum},#{labEpType},#{stealth},#{createTime},#{updateTime})
	</insert>
</mapper>